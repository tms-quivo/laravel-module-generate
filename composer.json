{
    "name": "tomosia-module/laravel-module-generate",
    "description": "Helps quickly generate common Laravel classes (like Controller, Request, etc.) via Artisan commands",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1|^8.2",
        "laravel/framework": "^10.0|^11.0",
        "livewire/livewire": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Tomosia\\LaravelModuleGenerate\\": "src/"
        },
        "files": [
            "src/Helpers/Helper.php"
        ]
    },
    "authors": [
        {
            "name": "Qui Vo",
            "email": "qui.vo@tomosia.com",
            "role": "Developer"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Tomosia\\LaravelModuleGenerate\\LaravelModuleGenerateProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/pint": "^1.22"
    }
}
